<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory
        android:key="pref_key_nauta_settings"
        android:title="Configuración Correo Nauta">
        <EditTextPreference
            android:defaultValue="pepe@nauta.cu"
            android:key="pref_key_nauta_user"
